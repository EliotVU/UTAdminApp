{
    "name": "UTAdmin",
    "version": "0.0.0",
    "description": "An admin interface for Unreal Tournament",
    "main": "gulpfile.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "devDependencies": {
        "bower": "^1.3.12",
        "traceur": "0.0.79",
        "gulp": "^3.8.10",
        "gulp-traceur": "^0.14.1",
        "gulp-sourcemaps": "^1.2.8",
        "gulp-concat": "^2.4.2",
        "gulp-browserify": "^0.5.0",
        "es6ify": "^1.5.1",
        "browserify": "^7.0.0",
        "vinyl-transform": "^1.0.0",
        "browserify-shim": "^3.8.1",
        "vinyl-source-stream": "^1.0.0"
    },
    "browserify": {
        "shim": {
            "bootstrap": {
                "path": "src/app/vendor/bootstrap/dist/js/bootstrap.min.js"
            },
            "traceur-runtime": {
                "path": "src/app/vendor/traceur-runtime/traceur-runtime.min.js",
                "exports": "$traceurRuntime"
            },
            "angular": {
                "path": "src/app/vendor/angular/angular.min.js",
                "exports": "angular"
            }
        }
    }
}
